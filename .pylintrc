[MASTER]
extension-pkg-whitelist=
    lxml,
    tesserocr


[TYPECHECK]
ignored-classes=ElementMaker


[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    too-many-arguments,
    too-many-instance-attributes,
    too-many-positional-arguments,
    consider-using-f-string,
    duplicate-code,
    invalid-name
